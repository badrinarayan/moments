Universe = vanilla
Getenv = True
+WantRHEL6Job = True
Requirements = MachineOwner == "Optimization"
# && TARGET.FileSystemDomain==MY.FileSystemDomain
Rank = kflops
notify_user = badrinarayan+condor@gmail.com
InputDir = /data/badri/moments
OutputDir = /data/badri/moments
should_transfer_files = true 
