Output = $(OutputDir)/stdout/stdout.$(fileNo)
transfer_input_files = admm_ben_general.m,prony_pencil_est.m,poles_amps.m,compare.m,SpaRSA.m,cadzow_denoise.m,moment_pad.m,moment_sparsa.m,toeplitz_approx.m
+transfer_input = "inputs/equispaced/input$(fileNo).mat"
transfer_output_files = output$(fileNo).mat
Arguments = compare($(fileNo))
WhenToTransferOutput = ON_EXIT
copy_to_spool = False

Queue 480
